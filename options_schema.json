{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "options": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "cmd": {
            "type": "string",
            "description": "Command or tool related to the option",
            "enum": ["logger", "telemetry"]
          }
        },
        "required": ["cmd"],
        "additionalProperties": false
      }
    }
  },
  "required": ["options"]
}
